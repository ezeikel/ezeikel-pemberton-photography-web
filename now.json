{
  "version": 2,
  "name": "ezeikel-pemberton-photography-web",
  "public": true,
  "builds": [
    { "src": "api/serverless.js", "use": "@now/node-server" },
    { "src": "dist/browser/**/*", "use": "@now/static" }
  ],
  "routes": [
    { "src": "/(.*[.].+)", "dest": "dist/browser/$1" },
    { "src": "/(.*)", "dest": "api/serverless.js" }
  ]
}
