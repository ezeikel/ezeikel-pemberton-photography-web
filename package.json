{
  "name": "ezeikel-pemberton-photography-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:dev": "yarn build:ssr && yarn serve:ssr",
    "build": "ng build",
    "build:prod": "ng build --prod --no-progress",
    "test": "ng test --karma-config src/karma.conf.js",
    "test:prepush": "ng test --karma-config src/karma.conf.js --watch=false --browsers ChromeHeadless",
    "lint": "ng lint",
    "lint-staged": "lint-staged",
    "pretty-quick": "pretty-quick",
    "e2e": "ng e2e",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "build:ssr": "yarn build:client-and-server-bundles && yarn compile:server",
    "build:client-and-server-bundles": "ng build --prod && ng run ezeikel-pemberton-photography-web:server:production --bundleDependencies all"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && lint-staged && ng lint --fix"
    }
  },
  "browsersList": [
    "> 1%",
    "last 2 versions"
  ],
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "^0.803.5",
    "@angular/animations": "^8.2.7",
    "@angular/common": "^8.2.7",
    "@angular/compiler": "^8.2.7",
    "@angular/core": "^8.2.7",
    "@angular/forms": "^8.2.7",
    "@angular/http": "^7.2.15",
    "@angular/platform-browser": "^8.2.7",
    "@angular/platform-browser-dynamic": "^8.2.7",
    "@angular/platform-server": "^8.2.7",
    "@angular/router": "^8.2.7",
    "@nguniversal/express-engine": "^8.1.1",
    "@nguniversal/module-map-ngfactory-loader": "8.1.1",
    "contentful": "^7.10.0",
    "core-js": "^3.2.1",
    "express": "^4.15.2",
    "font-awesome": "^4.7.0",
    "lint-staged": "^9.4.2",
    "normalize.css": "^8.0.1",
    "popper.js": "^1.15.0",
    "rxjs": "^6.5.3",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular/cli": "^8.3.5",
    "@angular/compiler-cli": "^8.2.7",
    "@angular/language-service": "^8.2.7",
    "@types/jasmine": "~3.4.0",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "~12.7.5",
    "codelyzer": "~5.1.1",
    "husky": "^3.0.5",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "protractor": "~5.4.2",
    "ts-loader": "^5.2.0",
    "ts-node": "~8.4.1",
    "tslint": "~5.20.0",
    "typescript": "^3.5.3",
    "webpack-cli": "^3.1.0"
  }
}